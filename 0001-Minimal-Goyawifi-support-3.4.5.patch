+static void gps_eclk_ctrl(int on)
+{
+	mfp_cfg_t gps_eclk_en = GPIO089_GPS_CLK;
+	mfp_cfg_t gps_eclk_dis = GPIO089_AP_AGPS_CLK_DIS;
+
+	if (on)
+		mfp_config(&gps_eclk_en, 1);
+	else
+		mfp_config(&gps_eclk_dis, 1);
+}
