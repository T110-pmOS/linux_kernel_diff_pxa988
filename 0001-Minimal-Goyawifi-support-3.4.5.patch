+/*
+ * PXA988 USB DEVICE PHY(UTMI) Registers
+ */
+struct pxa988_usb_phy {
+	u16 utmi_id;		/* 0x00 */
+	u16 pad0;
+	u16 utmi_pll_reg0;	/* 0x04 */
+#define PLLVDD18(x)		((x & 0x3) << 14)
