+			       "|%14s\t|%5s\t|\n", "SDH ACLK", "off");
+
+	if ((((reg & (1 << 4)) >> 4) == 1) && (((reg & (1 << 1)) >> 1) == 1))
+		len += snprintf(buf + len, size,
+			       "|%14s\t|%5s\t|\n", "SDH0 FCLK", "on");
+	else
+		len += snprintf(buf + len, size,
+			       "|%14s\t|%5s\t|\n", "SDH0 FCLK", "off");
+
+	reg = __raw_readl(APMU_SDH1);
+	if ((((reg & (1 << 4)) >> 4) == 1) && (((reg & (1 << 1)) >> 1) == 1))
+		len += snprintf(buf + len, size,
+			       "|%14s\t|%5s\t|\n", "SDH1 FCLK", "on");
