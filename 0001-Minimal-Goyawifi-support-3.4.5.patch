+#if defined(CONFIG_QUICKLOGIC_BRIDGE)
+	if (device_create_file(&vx5b3dInfo->lcd->dev, &dev_attr_veeregwrite) < 0)
+		printk("Failed to create device file(%s)!\n", dev_attr_veeregwrite.attr.name);
