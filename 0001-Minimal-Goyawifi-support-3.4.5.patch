+static int sdhc_clk_enable(struct clk *clk)
+{
+	CLK_SET_BITS(SDH_FCLK_EN, 0);
+	CLK_SET_BITS(SDH_FCLK_REQ, 0);
+	return 0;
+}
+
+static void sdhc_clk_disable(struct clk *clk)
+{
+	CLK_SET_BITS(0, SDH_FCLK_EN);
+
+}
