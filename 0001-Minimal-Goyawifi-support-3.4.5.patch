+	ocv = ocv_table[v3];
+	dev_dbg(info->dev, "%s, ocv: %d , v1:%d, v2:%d, v3:%d \n",
+		__func__, ocv, v1, v2, v3);
+
+	/* (mV/m-Ohm) = A , so, the ib current unit is A with factor */
