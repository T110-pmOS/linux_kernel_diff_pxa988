+struct pxa9xx_acipc {
+	struct resource *mem;
+	void __iomem *mmio_base;
+	int irq[ACIPC_NUMBER_OF_INTERRUPTS];
+	char irq_name[ACIPC_NUMBER_OF_INTERRUPTS][20];
+	u32 IIR_val;
