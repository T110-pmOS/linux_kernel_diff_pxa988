+	if (panel_id == HX8369B_PANEL_BOE3) {
+		ktd_lcd_backlight_data.brt_table = ktd_brt_table_boe;
+		ktd_lcd_backlight_data.sz_table = ARRAY_SIZE(ktd_brt_table_boe);
+	}
+	platform_device_register(&ktd_device);
+#endif
