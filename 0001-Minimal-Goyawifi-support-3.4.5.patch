+static unsigned long pxa988_ddraxi_getrate(struct clk *clk)
+{
+	if (cur_ddraxi_op)
+		return cur_ddraxi_op->dclk * MHZ;
+	else
+		pr_err("%s: cur_ddraxi_op NULL\n", __func__);
+
+	return 0;
+}
