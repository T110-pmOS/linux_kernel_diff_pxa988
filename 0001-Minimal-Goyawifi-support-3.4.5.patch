+		else if(!strcmp(cmd, "debug0x4f4c"))
+			pm800_extern_write(PM80X_BASE_PAGE, PMIC_GENERAL_USE_REGISTER, PMIC_GENERAL_USE_BOOT_BY_DEBUGLEVEL_LOW);
+		else if(!strcmp(cmd, "debug0x494d"))
