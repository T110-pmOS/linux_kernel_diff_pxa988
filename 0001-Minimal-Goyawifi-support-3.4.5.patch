+static void gc_aclk_init(struct clk *clk)
+{
+	__clk_fill_periph_tbl(clk, gc_aclk_tbl,
+		ARRAY_SIZE(gc_aclk_tbl));
+	/* default GC aclk = 312M sel = pll1_624, div = 2 */
+	__clk_periph_init(clk, &pll1_624, 2, 1);
