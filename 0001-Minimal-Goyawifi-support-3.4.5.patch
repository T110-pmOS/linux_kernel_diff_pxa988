+void pxa988_reserve_fb_mem(void)
+{
+	u32 fb_size_align;
+
+	if (!skip_power_on)
+		return;
+
+	/* make sure that reserved base address and size is MB aligned */
+	if (DEFAULT_FB_SIZE != (DEFAULT_FB_SIZE & (~0xfffff)))
+		fb_size_align = (DEFAULT_FB_SIZE & (~0xfffff)) + 0x100000;
