+static unsigned long lcd_func_clk_getrate(struct clk *clk)
+{
+	unsigned int mux, div;
+	__clk_get_mux_div(clk, &mux, &div);
+	div = div - 1;
+	if (0 == div || (clk->parent == &pll3))
+		div = 1;
+	return clk_get_rate(clk->parent) / div;
+}
