+static int clk_pll2p_setrate(struct clk *clk, unsigned long rate)
+{
+	unsigned int div_val;
+	union apb_spare_pllswcr pll2_sw_ctrl;
+	unsigned long flags;
+
+	if (__pll2_is_enabled())
+		return -EPERM;
