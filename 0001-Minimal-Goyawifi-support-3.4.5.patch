+		data = pm822_extern_read(PM822_BASE_PAGE, PM822_WAKEUP2);
+		pr_info("PM822_WAKEUP2 Reg(0x%02x) is 0x%02x\n", PM822_WAKEUP2,data);
+		data &= ~(PM822_WD_TIMER_ACT_MASK);
