+		i += scnprintf(buf + i, PAGE_SIZE - i, "%d\n",
+			       (battery->event & EVENT_MUSIC) ? 1 : 0);
+		break;
+	case BATT_EVENT_VIDEO:
+		i += scnprintf(buf + i, PAGE_SIZE - i, "%d\n",
