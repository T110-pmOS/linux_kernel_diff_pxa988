+struct pm822_rtc_info {
+	struct pm822_chip *chip;
+	struct regmap *map;
+	struct rtc_device *rtc_dev;
+	struct device *dev;
+	struct delayed_work calib_work;
+
+	int irq;
+	int vrtc;
+	int (*sync) (unsigned int ticks);
+};
+
+#if defined(CONFIG_RTC_CHN_ALARM_BOOT)
+#if defined(CONFIG_BATTERY_SAMSUNG)
