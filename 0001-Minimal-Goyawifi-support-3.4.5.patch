+		atomic_inc(&data->is_disabled);
+	else
+		atomic_dec(&data->is_disabled);
+
+	dev_info(dev, "[WARNING]ddr fc is %s from debug interface!\n",\
+		atomic_read(&data->is_disabled) ? "disabled" : "enabled");
+	return size;
