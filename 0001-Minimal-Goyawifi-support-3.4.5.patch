+static struct page *
+__alloc_contig_migrate_alloc(struct page *page, unsigned long private,
+			     int **resultp)
+{
+	gfp_t gfp_mask = GFP_USER | __GFP_MOVABLE | __GFP_CMA;
+
+	if (PageHighMem(page))
+		gfp_mask |= __GFP_HIGHMEM;
+
