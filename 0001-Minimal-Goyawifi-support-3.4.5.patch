+		len += snprintf(p + len, buf_len - len,
+				"SCU Configuration: 0x%08x\n", value);
+
+		value = readl_relaxed(SCU_VIRT_BASE + SCU_CPU_STATUS);
+		len += snprintf(p + len, buf_len - len,
