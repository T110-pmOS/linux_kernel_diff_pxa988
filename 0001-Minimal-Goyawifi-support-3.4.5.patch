+{
+	int ret;
+
+	ret = platform_device_register(&standalone_hotplug_device);
+
+	if (ret) {
+		pr_err("Register device Failed\n");
+		return ret;
+	}
+
+	printk(KERN_INFO "standalone_hotplug_device_init: %d\n", ret);
+
+	return ret;
+}
