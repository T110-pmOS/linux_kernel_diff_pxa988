+	BUG_ON(!pll2_default);
+	clk->rate = pll2_default;
+	if (__pll2_is_enabled()) {
+		__get_pll2_freq(&pll2, &pll2p);
+		pr_info("PLL2 is already enabled @ %lu\n",
