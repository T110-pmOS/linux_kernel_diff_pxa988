+			data |= PMIC_GENERAL_USE_BOOT_BY_RECOVERY_DONE;
+			pm822_extern_write(PM822_BASE_PAGE, PMIC_GENERAL_USE_REGISTER, data);
+		} else if (!strcmp(cmd, "arm11_fota")) {
+			data |= PMIC_GENERAL_USE_BOOT_BY_FOTA;
+			pm822_extern_write(PM822_BASE_PAGE, PMIC_GENERAL_USE_REGISTER, data);
