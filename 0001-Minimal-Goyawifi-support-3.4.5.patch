+#define GET_SUB_CABLE_TYPE(extended)	\
+	((extended >> ONLINE_TYPE_SUB_SHIFT)&0xf)
+#define GET_POWER_CABLE_TYPE(extended)	\
+	((extended >> ONLINE_TYPE_PWR_SHIFT)&0xf)
+
+#endif /* __SEC_CHARGING_COMMON_H */
