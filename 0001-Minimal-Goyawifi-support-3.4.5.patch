+static inline void clk_get_lock(struct clk *clk)
+{
+	if (clk->cansleep)
+		mutex_lock(&clk->mutex);
+	else
+		spin_lock(&clk->spinlock);
+}
+
+static inline void clk_release_lock(struct clk *clk)
+{
+	if (clk->cansleep)
+		mutex_unlock(&clk->mutex);
+	else
+		spin_unlock(&clk->spinlock);
+}
