+	/* already get pmu_lock in LPM code */
+	awucrm = __raw_readl(MPMU_AWUCRM);
+	apcr = __raw_readl(MPMU_APCR);
+	__raw_writel(awucrm & ~PMUM_WAKEUP2, MPMU_AWUCRM);
+	__raw_writel(apcr | PMUM_SLPWP2, MPMU_APCR);
