+		.from = &pxa988_dvfs_rail_ap_lpm,
+		.to = &pxa988_dvfs_rail_ap_active,
+		.solve = vcc_main_solve_lpm,
+	},
+	{
+		.from = &pxa988_dvfs_rail_apsub_idle,
+		.to = &pxa988_dvfs_rail_ap_active,
+		.solve = vcc_main_solve_lpm,
+	},
+};
