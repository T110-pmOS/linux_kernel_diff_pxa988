+	trans_load_##_name = tmp; \
+	return count; \
+}
+#define define_show_thhd_function(_name) \
+static ssize_t load_get_##_name(struct device *dev, \
+				struct device_attribute *attr, \
+				char *buf) \
+{ \
+	return sprintf(buf, "%d\n", (int) trans_load_##_name); \
+}
