+static unsigned int pxa168fb_rw_all_regs(struct pxa168fb_info *fbi,
+		unsigned int *regs, int is_read)
+{
+	u32 i;
+	u32 reg;
+
+	for (i = 0xC0; i <= 0x01C4; i += 4) {
+		if (is_read) {
+			reg = readl(fbi->reg_base + i);
+			regs[i] = reg;
+		} else {
+			writel(regs[i], fbi->reg_base + i);
