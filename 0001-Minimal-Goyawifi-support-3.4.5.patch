+static void esd_timeout_handler(unsigned long data)
+{
+	struct bt532_ts_info *info = (struct bt532_ts_info *)data;
+
+	info->p_esd_timeout_tmr = NULL;
+	queue_work(esd_tmr_workqueue, &info->tmr_work);
